
{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
      ],

    "rules": {
        "semi": ["error", "always"],
        
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    },
    "plugins": [
        "react"
      ],
      "parser":"babel-eslint",
      "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },

      "env": {
        "browser": true,
        "node": true
      }
}